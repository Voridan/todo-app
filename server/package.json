{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.ts",
  "scripts": {
    "start": "rimraf dist && tsc-watch --onSuccess \"npm run watch\"",
    "watch": "nodemon src/index.ts --watch ./src",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "typeorm migration:create ./src/infrastructure/database/migrations/typeorm/migration",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d ./src/infrastructure/database/config/typeorm/dataSource.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d ./src/infrastructure/database/config/typeorm/dataSource.ts",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate ./src/infrastructure/database/migrations/typeorm/migration -d ./src/infrastructure/database/config/typeorm/dataSource.ts",
    "schema:sync": "typeorm schema:sync -- -d ./src/infrastructure/database/config/typeorm/dataSource.ts",
    "migration:show": "typeorm migration:show -- -d ./src/infrastructure/database/config/typeorm/dataSource.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.0",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.17"
  }
}
